</head>
<body>
    <script src="js/buttons.js"></script>
    <table class="tableData" id="tableData">
        <thead>
            <tr>
                <td>id</td>
                <td>Имя</td>
                <td>Фамилия</td>
                <td>Отчество</td>
                <td>Дата рождения</td>
                <td>Должность</td>
                <td>Стаж</td>
                <td></td>
            </tr>
        </thead>
        <% for(var i = 0; i< result.length; i++){ %>
            <tr>
                <td><%= result[i]._id%></td>
                <td><%= result[i].name%></td>
                <td><%= result[i].family%></td>
                <td><%= result[i].patronymic%></td>
                <td><%= result[i].birth%></td>
                <td><%= result[i].position%></td>
                <td><%= result[i].experience%></td>
                <td class="buttons">
                    <input type="button" value="Удалить" id="<%= result[i]._id%>" class="delete writeData" onclick="buttonsDelete(this)">
                    <input type="button" value="Изменить" id="<%= result[i]._id%>" class="chenge writeData" onclick="buttonsChange(this)">
                </td>
            </tr>
        <% } %>
